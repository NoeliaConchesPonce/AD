<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida.ae5">
	<class name="Llibre" table="llibres">
		<id name="identificador" column="Id">
			<generator class="increment" />
		</id>
		<property name="titol" type="string" column="Titol" />
		<property name="autor" type="string" column="Autor" />
		<property name="anyNaixement" type="int"
			column="Any_de_naixement" />
		<property name="anyPublicacio" type="int"
			column="Any_de_publicacio" />
		<property name="editorial" type="string" column="Editorial" />
		<property name="nombrePagines" type="int" column="NombrePags" />
	</class>
</hibernate-mapping>
